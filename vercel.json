{
  "version": 2,
  "builds": [
      {
          "src": "index.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "index.js"
      }
  ],
  "rewrites": [
      {
          "source": "/(.*)",
          "destination": "/index.js"
      }
  ],
  "functions": {
      "api/*.js": {
          "runtime": "nodejs@20.9.0"
      }
  }
}